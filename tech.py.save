
from flask import Flask, render_template

app = Flask(__name__)

@app.route("/")
def home():
    return render_template("home.html")

@app.route("/about")
def about():
    return render_template("about.html")
@app.route('/form')
def userform():
    name = ["bashir","akankiza","bella"]
    return render_template("form.html", username=name)
if __name__ == "__main__":
    app.run(debug=True)
